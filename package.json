{
  "name": "electron-vue-vite-ts-template",
  "version": "1.0.0",
  "description": "A minimal Electron + Vue application",
  "main": "build/main/main.js",
  "scripts": {
    "watch": "tsc -w",
    "serve": "node scripts/dev-server.js",
    "build": "node scripts/build.js && electron-builder",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux",
    "electronegativity": "electronegativity -i ./src",
    "lint": "eslint --ext .js,.vue src"
  },
  "repository": "https://github.com/RvD-Projects/electron-vue-vite-ts",
  "author": {
    "name": "Izocel",
    "url": "https://github.com/Izocel"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/cli": "^5.0.8",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "electron": "^21.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.7.0",
    "prettier": "2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "vue": "^3.2.40"
  }
}
